    </head>
    <body>
        <div class="main">
            <!--Start of the page -->
            <nav>
                <!--Header part -->
                <div class="brand">
                    <span>DOWNTOWN ALLEY</span>
                </div>

            </nav>
            <div id="sidebar">
                <!-- Left side of the page -->
                <div class="profile">
                    <div class="profile-photo">
                        <!--Insert code of Image depends on their role -->
                    </div>
                    <div class="profile-username">
                        <center><h4>Manager</h4></center> <!--Can be change according to its username -->
                    </div>
                </div>
                <div id="navlist">
                    <div class="item item-nav">
                            <a href="@Url.Action("Index", "InventoryManagement")" target="_blank"><span> Sales Dashboard</span><i class="fa-solid fa-chevron-right dropdown"></i></a>
                            <div class="subitem">
                                <a href="https://www.facebook.com" target="_blank"><span>Related 1</span></a>
                                <a href="https://www.youtube.com/watch?v=dQw4w9WgXcQ&pp=ygUXbmV2ZXIgZ29ubmEgZ2l2ZSB5b3UgdXA%3D" target="_blank"><span>Related 2</span></a>
                                <a href="#" target="_blank"><span>Related 3</span></a>
                            </div>
                    </div>
                    <div class="item"><a href="@Url.Action("Index", "InventoryManagement")" target="_blank"><span>Profit</span><i class="fa-solid fa-chevron-right"></i></a></div>
                    <div class="item"><a href=""><span>Losses</span><i class="fa-solid fa-chevron-right"></i></a></div>
                    <div class="item"><a href=""><span>Audit Trail</span><i class="fa-solid fa-chevron-right"></i></a></div>
                    <div class="item"><a href=""><span>Accounts</span><i class="fa-solid fa-chevron-right"></i></a></div>
                    <div class="item"><a href=""><span>Inventory</span><i class="fa-solid fa-chevron-right"></i></a></div>

                </div>

            </div>
            <main class="main-content"><!--Start Renderbody of the page -->
                @RenderBody()
            </main><!--End Renderbody of the page -->
        </div>
        <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
        <script src="https://cdn.jsdelivr.net/npm/popper.js@1.12.9/dist/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>


        
